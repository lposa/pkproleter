{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "strictNullChecks": false,
    "isolatedModules": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    ".eslintrc.js",
    "babel.config.js"
  ],
  "exclude": ["__tests__/**/*-test.ts", "node_modules"],
}
